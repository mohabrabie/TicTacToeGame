<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="585.0" prefWidth="875.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LevelsController">
   <children>
      <ImageView fitHeight="585.0" fitWidth="875.0" layoutY="1.0" pickOnBounds="true">
         <image>
            <Image url="@../icons/blackone.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="206.0" layoutY="49.0" prefHeight="488.0" prefWidth="478.0">
         <children>
            <ImageView fitHeight="488.0" fitWidth="499.0" pickOnBounds="true">
               <image>
                  <Image url="@../icons/grayone.jpg" />
               </image>
            </ImageView>
            <JFXButton fx:id="hardButton" layoutX="190.0" layoutY="138.0" onAction="#hardButtonAction" prefHeight="54.0" prefWidth="138.0" style="-fx-background-color: #FCFC04;" text="Hard">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="mediumButton" layoutX="192.0" layoutY="236.0" onAction="#mediumButtonAction" prefHeight="54.0" prefWidth="138.0" style="-fx-background-color: #FCFC04;" text="Medium">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="easyButton" layoutX="190.0" layoutY="328.0" onAction="#easyButtonAction" prefHeight="54.0" prefWidth="144.0" style="-fx-background-color: #FCFC04;" text="Easy">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </JFXButton>
            <Label layoutX="152.0" layoutY="44.0" text="Choose Level!">
               <font>
                  <Font name="System Bold Italic" size="34.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
	  <JFXButton fx:id="backButton" layoutX="14.0" layoutY="14.0" onAction="#backButtonAction" style="-fx-background-color: #FCFC04;" text="Back">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
      </JFXButton>
   </children>
</AnchorPane>
